{
    "name": "vistara-functions",
    "version": "1.0.0",
    "description": "Cloud Functions for Vistara/LokalNexus AI Orchestrator",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "18"
    },
    "dependencies": {
        "@google/genai": "^1.31.0",
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.14.0",
        "typescript": "~5.8.2"
    },
    "private": true
}